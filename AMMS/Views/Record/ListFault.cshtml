                <td>
                    @Html.DisplayFor(modelItem => item.AircraftId)
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditFault", new { id = item.Id }) |
                    @Html.ActionLink("Details", "DetailsFault", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "DeleteFault", new { id = item.Id })
                </td>
            </tr>
        }
    </tbody>
</table>
