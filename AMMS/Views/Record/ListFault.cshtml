@model IEnumerable<AMMS.Models.ViewModels.FaultViewModel>

@{
    ViewData["Title"] = "Fault List";
}

<h2>Fault List</h2>

<p>
    <a asp-action="CreateFault" asp-route-parentId="@ViewBag.ParentId">Create New Inspection</a>
</p>
<table class="table">
    <thead>
