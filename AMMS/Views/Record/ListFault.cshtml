                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SystemCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FaultDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FaultNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FaultTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiscPID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FaultText)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiscAcftHrs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WhenDisc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HowRecog)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MalEff)
