            <th>
                @Html.DisplayNameFor(model => model.CompAcftHrs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rounds)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ActionCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompWUC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Action)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompPID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompCat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompHrs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AircraftId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AcftSerialNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AcftModel)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Status)
                </td>
